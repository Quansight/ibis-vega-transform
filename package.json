{
  "name": "ibis-vega-transform",
  "version": "2.0.1",
  "description": "A JupyterLab extension for performing Vega transforms lazily using Ibis.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/Quansight/ibis-vega-transform",
  "bugs": {
    "url": "https://github.com/Quansight/ibis-vega-transform/issues"
  },
  "license": "Apache-2.0",
  "author": "OmniSci/Quansight",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/Quansight/ibis-vega-transform.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepublish": "jlpm run clean && jlpm run build",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "tslint": "tslint -c tslint.json --project tsconfig.json '**/*{.ts,.tsx}'",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.4",
    "@jupyterlab/notebook": "^1.0.4",
    "@jupyterlab/rendermime": "^1.0.4",
    "@jupyterlab/services": "^4.0.4",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/widgets": "^1.9.0",
    "jupyter-jaeger": "^1.0.1",
    "vega": "5.4.0",
    "vega-embed": "4.2.4",
    "vega-lite": "3.4.0"
  },
  "peerDependencies": {
    "phoila": "^0.3.0"
  },
  "devDependencies": {
    "phoila": "^0.3.0",
    "@types/jaeger-client": "3.15.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
