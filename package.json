{
  "name": "jupyterlab-mapd",
  "version": "0.4.0",
  "description": "JupyterLab - MapD backend renderer",
  "homepage": "https://github.com/Quansight/jupyter-mapd-renderer",
  "bugs": {
    "url": "https://github.com/Quansight/jupyter-mapd-renderer/issues"
  },
  "license": "Apache-2.0",
  "author": "MapD",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Quansight/jupyter-mapd-renderer.git"
  },
  "scripts": {
    "build": "tsc && cp src/browser-connector.js lib/",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.17.0-1",
    "@jupyterlab/apputils": "^0.17.0-1",
    "@jupyterlab/coreutils": "^2.0.0-1",
    "@jupyterlab/docregistry": "^0.17.0-1",
    "@jupyterlab/launcher": "^0.17.0-1",
    "@jupyterlab/mainmenu": "^0.6.0-1",
    "@jupyterlab/rendermime-interfaces": "^1.0.6",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/datagrid": "^0.1.5",
    "@phosphor/widgets": "^1.5.0",
    "vega": "3.x",
    "vega-lite": "2.x"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "^2.9.1"
  },
  "jupyterlab": {
    "extension": "lib/extension",
    "mimeExtension": "lib/mimextension",
    "schemaDir": "schema"
  }
}
