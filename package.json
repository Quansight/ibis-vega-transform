{
  "name": "jupyterlab-mapd",
  "version": "0.5.1",
  "description": "JupyterLab - MapD backend renderer",
  "homepage": "https://github.com/Quansight/jupyter-mapd-renderer",
  "bugs": {
    "url": "https://github.com/Quansight/jupyter-mapd-renderer/issues"
  },
  "license": "Apache-2.0",
  "author": "MapD",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Quansight/jupyter-mapd-renderer.git"
  },
  "scripts": {
    "build": "tsc && cp src/browser-connector.js lib/",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.css,.json,.md}'",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.18.0",
    "@jupyterlab/apputils": "^0.18.0",
    "@jupyterlab/codeeditor": "^0.18.0",
    "@jupyterlab/completer": "^0.18.0",
    "@jupyterlab/coreutils": "^2.1.0",
    "@jupyterlab/docregistry": "^0.18.0",
    "@jupyterlab/launcher": "^0.18.0",
    "@jupyterlab/mainmenu": "^0.7.0",
    "@jupyterlab/rendermime-interfaces": "^1.1.3",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/datagrid": "^0.1.5",
    "@phosphor/widgets": "^1.6.0",
    "vega": "3.x",
    "vega-lite": "2.x"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "rimraf": "~2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.1"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jupyterlab": {
    "extension": "lib/extension",
    "mimeExtension": "lib/mimextension",
    "schemaDir": "schema"
  }
}
