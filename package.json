{
  "name": "ibis-vega-transform",
  "version": "5.2.2",
  "description": "A JupyterLab extension for performing Vega transforms lazily using Ibis.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/Quansight/ibis-vega-transform",
  "bugs": {
    "url": "https://github.com/Quansight/ibis-vega-transform/issues"
  },
  "license": "Apache-2.0",
  "author": "OmniSci/Quansight",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/Quansight/ibis-vega-transform.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension && jlpm clean:modules",
    "clean:labextension": "rimraf ibis_vega_transform/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:modules": "rimraf node_modules",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "prepublish": "jlpm run clean && jlpm run build",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "tslint": "tslint -c tslint.json --project tsconfig.json '**/*{.ts,.tsx}'",
    "test": "env JEST_PUPPETEER_CONFIG=./config/jest/jest-puppeteer.config.js jest --runInBand --verbose=true --detectOpenHandles --config ./config/jest/jest.config.js",
    "test:debug": "env HEADLESS=false SLOWMO=true jlpm test",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/docregistry": "^3.0.0",
    "@jupyterlab/notebook": "^3.0.0",
    "@jupyterlab/rendermime-interfaces": "^3.0.0",
    "@jupyterlab/services": "^6.0.5",
    "@lumino/coreutils": "^1.4.2",
    "@lumino/widgets": "^1.11.1",
    "jupyter-jaeger": "^1.0.1",
    "vega": "^5.13.0",
    "vega-embed": "^6.10.0",
    "vega-lite": "^4.13.1"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^3.3.2",
    "@types/jaeger-client": "3.15.3",
    "@types/jest": "^24.0.25",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/node": "^13.1.2",
    "@types/puppeteer": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "jest-environment-puppeteer-jsdom": "^4.3.1",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.17.0",
    "puppeteer": "^2.0.0",
    "resemblejs": "^3.2.3",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.0.0",
    "typescript": "~3.8.3",
    "@jupyterlab/builder": "^3.0.5"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip",
          "conda"
        ],
        "base": {
          "name": "ibis-vega-transform"
        }
      }
    },
    "extension": true,
    "sharedPackages": {
      "@material-ui/core": {
        "singleton": true
      },
      "@material-ui/icons": {
        "singleton": true
      },
      "@material-ui/lab": {
        "singleton": true
      }
    },
    "outputDir": "ibis_vega_transform/labextension"
  },
  "husky": {
    "hooks": {
      "pre-commit": "black ibis_vega_transform && jlpm run prettier"
    }
  }
}
